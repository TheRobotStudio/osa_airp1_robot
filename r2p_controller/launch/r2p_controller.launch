<?xml version="1.0" ?>
<launch>
  <!-- Launch Gazebo  -->
  <include file="$(find r2p_controller)/launch/seven_dof_arm_world.launch" />   
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find r2p_controller)/r2p_controller.yaml" command="load"/>
	<!-- Spaw the controller -->
  <node name="r2p_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" args="r2p_controller_name"/>
</launch>
